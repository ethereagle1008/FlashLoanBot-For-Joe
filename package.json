{
  "name": "foundry-project",
  "version": "1.0.0",
  "description": "**Foundry is a blazing fast, portable and modular toolkit for Ethereum application development written in Rust.**",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "compile": "forge build",
    "clean": "forge clean",
    "slither": "slither .",
    "test": "npm run test:unit",
    "test:unit": "forge test --match-path ./test/** -vv",
    "test:deploy": "forge test --match-path ./test/FlashbotFactoryTest.t.sol -vv",
    "test:deploy_sepolia": "forge test --match-path ./test/FlashbotFactorytest.t.sol -vv --match-test testSepoliaDeploy",
    "deploy": "forge create --private-key YOUR-PRIVATE-KEY --rpc-url ${SEPOLIA_RPC_URL} --verify --etherscan-api-key YOUR-ETHERSCAN-API-KEY src/Flashbot.sol:Flashbot",
    "deployByScript": "forge script script/Flashbot.s.sol:FlashbotDeploy --broadcast --verify src/Flashbot.sol:Flashbot --rpc-url https://eth-sepolia.g.alchemy.com/v2/dF3pLDZtI7_HZv7dbnWOIWaLTdy2rKTl -vv",
    "start": "node index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async-request": "^1.2.0",
    "bignumber.js": "^9.0.1",
    "dotenv": "^8.2.0",
    "log4js": "^6.4.4",
    "web3": "^1.3.4"
  }
}
